@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix bind: <http://purl.org/semantictools/v1/vocab/bind#> .
@prefix ltim: <http://www.imsglobal.org/imspurl/lti/v2/vocab/message#> .
ltim:LtiLink rdf:type bind:EmbeddableClass .
ltim:LtiLink rdfs:subClassOf
  [ rdf:type bind:AssociationQualifier ;
    bind:onProperty ltim:handler ;
    bind:associationType bind:Aggregation
  ] .
ltim:LtiLink rdfs:subClassOf
  [ rdf:type bind:AssociationQualifier ;
    bind:onProperty ltim:extension ;
    bind:associationType bind:Composition
  ] .
ltim:Property rdf:type bind:EmbeddableClass .
ltim:PropertySet rdf:type bind:EmbeddableClass .
ltim:PropertySet rdfs:subClassOf
  [ rdf:type bind:AssociationQualifier ;
    bind:onProperty ltim:property ;
    bind:associationType bind:Composition
  ] .
ltim:PlatformPropertySet rdf:type bind:EmbeddableClass .
ltim:Platform rdf:type bind:EmbeddableClass .
ltim:LinkPlacement rdf:type bind:EmbeddableClass .
ltim:Credentials rdf:type bind:EmbeddableClass .
ltim:LinkLevelCredentials rdf:type bind:EmbeddableClass .

