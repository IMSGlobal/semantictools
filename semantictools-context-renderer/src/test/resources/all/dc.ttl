@prefix dc: <http://purl.org/dc/terms/> .
@prefix pcm: <http://purl.org/pearson/content/v1/vocab/metadata#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix topic: <http://purl.org/pearson/topics/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix bind: <http://purl.org/semantictools/v1/vocab/bind#> .

<http://purl.org/dc/terms/> a owl:Ontology  ;
  rdfs:label "Dublin Core" ;
  a bind:TargetNamespace ;
  bind:suggestedPrefix "dc" .

dc:created rdf:type rdf:Property ;
  rdfs:domain pcm:DublinCoreResource ;
  rdfs:range xsd:dateTime .

dc:description rdf:type rdf:Property ;
  rdfs:domain pcm:DublinCoreResource ;
  rdfs:range xsd:string .

dc:title rdf:type rdf:Property ;
  rdfs:domain pcm:DublinCoreResource ;
  rdfs:range xsd:normalizedString .

dc:subject rdf:type rdf:Property ;
  rdfs:domain pcm:DublinCoreResource ;
  rdfs:range topic:Topic .

